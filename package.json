{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --ignore-path .gitignore --write .",
    "fmt.check": "prettier --ignore-path .gitignore --check .",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.2",
    "@builder.io/qwik-city": "^1.4.2",
    "@fontsource/vt323": "^5.0.8",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/blocks": "7.6.10",
    "@storybook/builder-vite": "7.6.10",
    "@storybook/html": "7.6.10",
    "@storybook/html-vite": "7.6.10",
    "@types/eslint": "8.56.2",
    "@types/node": "^20.11.8",
    "@types/postcss-import": "^14.0.3",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "autoprefixer": "^10.4.17",
    "browserslist": "^4.22.3",
    "cssnano": "^6.0.3",
    "eslint": "8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-qwik": "^1.4.2",
    "include-media": "^2.0.0",
    "postcss-import": "^16.0.0",
    "postcss-load-config": "^5.0.2",
    "postcss-preset-env": "^9.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "3.2.4",
    "sass": "^1.70.0",
    "storybook": "7.6.10",
    "storybook-framework-qwik": "0.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "undici": "6.5.0",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1",
    "wrangler": "^3.25.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "^8.48.0"
      }
    }
  },
  "volta": {
    "node": "18.17.1"
  }
}
